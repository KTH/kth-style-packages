{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kth-style-web",
  "version": "2.0.0",
  "description": "KTH Style companion web app.",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
