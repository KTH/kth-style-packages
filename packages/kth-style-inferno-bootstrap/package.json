{
  "name": "kth-style-inferno-bootstrap",
  "version": "0.1.19",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-docs && node docs/server.js",
    "prepare": "npm run build",
    "postversion": "./scripts/version.sh",
    "build": "rm -rf ./dist && NODE_ENV=production babel lib --out-dir dist  --source-maps",
    "build-dev": "rm -rf ./dist && babel lib --out-dir dist --watch --source-maps",
    "build-docs": "rm -rf ./docs/dist && node_modules/.bin/webpack --config ./docs/webpack.config.js",
    "build-docs-dev": "rm -rf ./docs/dist && node_modules/.bin/webpack --watch --config ./docs/webpack.config.js"
  },
  "author": "Sebastian Ware <sebastian@urbantalk.se> (https://github.com/jhsware)",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "inferno-animation": "^0.4.11",
    "inferno-bootstrap": "^0.4.0",
    "inferno-component": "^3.10.1",
    "inferno-router": "^3.10.1",
    "inferno-popper": "^0.1.10",
    "kth-style": "^2.0.0-beta.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inferno": "^3.4.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.0.0-beta.3",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "history": "^4.6.3",
    "inferno": "^3.10.1",
    "inferno-component": "^3.10.1",
    "inferno-devtools": "^3.10.1",
    "inferno-router": "^3.10.1",
    "inferno-test-utils": "^3.10.1",
    "jest": "^20.0.4",
    "node-sass": "^4.7.2",
    "prismjs": "^1.10.0",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.8",
