{
  "presets": [
    "stage-0",
    "es2015"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-syntax-jsx",
    "add-module-exports",
    ["babel-plugin-inferno", {"imports": true}],
    ["module-resolver", {
      "alias": {
        "inferno-bootstrap/lib": "inferno-bootstrap/dist"
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-object-rest-spread",
        "babel-plugin-syntax-jsx",
        "add-module-exports",
        ["babel-plugin-inferno", {"imports": true}]
      ]
    }
  }
}
